<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ventrux.thescratchbtw" version="2.5" versionCode="8" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
